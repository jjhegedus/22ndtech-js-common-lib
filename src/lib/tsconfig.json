{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "noEmitHelpers": true,
      "declaration": true,
      "noImplicitAny": true,
      "suppressImplicitAnyIndexErrors": true,
      "skipLibCheck": true,
      "stripInternal": true,
      "lib": [
        "es2015",
        "dom"
      ]
    },
    "compileOnSave": false,
    "buildOnSave": false,
    "awesomeTypescriptLoaderOptions": {
      "forkChecker": true,
      "useWebpackText": true
    }
  }
  